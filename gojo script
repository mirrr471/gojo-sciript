Sound")) then sound.Volume=4;sound.RollOffMaxDistance=350;sound.RollOffMinDistance=5;else warn("사운드 로드 실패: "   .. tostring(sound.SoundId)   .. " (SoundName: "   .. tostring(soundName)   .. ")" );end else warn("사운드 ID 파싱 실패: "   .. tostring(sound.SoundId)   .. " (SoundName: "   .. tostring(soundName)   .. ")" );end end local function notify(title,text) local success,err=pcall(function() StarterGui:SetCore("SendNotification",{Title=title,Text=text,Duration=0.8});end);if (success==false) then warn("notify 에러: "   .. tostring(err) );end end local function shakeCamera(intensity,duration) local success,err=pcall(function() coroutine.wrap(function() local start=tick();while (tick() -start)<duration  do Camera.CFrame=Camera.CFrame * CFrame.new(math.random( -intensity,intensity) * 0.02 ,math.random( -intensity,intensity) * 0.02 ,0) ;RunService.RenderStepped:Wait();end end)();end);if (success==false) then warn("shakeCamera 에러: "   .. tostring(err) );end end local function moveCamera(startCFrame,endCFrame,duration,easingStyle) local success,err=pcall(function() local tweenInfo=TweenInfo.new(duration,easingStyle or Enum.EasingStyle.Sine ,Enum.EasingDirection.InOut);local tween=TweenService:Create(Camera,tweenInfo,{CFrame=endCFrame});tween:Play();return tween;end);if (success==false) then warn("moveCamera 에러: "   .. tostring(err) );end end local function createExplosion(position,color,size,damage) local explosionPart=Instance.new("Part");explosionPart.Shape=Enum.PartType.Ball;explosionPart.Size=Vector3.new(size,size,size);explosionPart.Position=position;explosionPart.Color=color;explosionPart.Transparency=0.3;explosionPart.Anchored=true;explosionPart.CanCollide=false;explosionPart.Parent=workspace;Debris:AddItem(explosionPart,1);local explosionParticle=Instance.new("ParticleEmitter");explosionParticle.Texture="rbxassetid://243098098";explosionParticle.Size=NumberSequence.new({NumberSequenceKeypoint.new(0,size/2 ),NumberSequenceKeypoint.new(1,0)});explosionParticle.Lifetime=Numbe